//按下Enter可跳轉到下一個textbox
private void SetKeyPress(object sender, KeyPressEventArgs e)
        {
            if (e.KeyChar == (char)Keys.Enter)  
            {
                SelectNextControl((Control)sender, true, true, true, true);
            }
            else
            {
                CommonBLL.SetEngNumKeyPress(e);
            }
        }
